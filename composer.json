{
    "name": "fairholm/elasticquent",
    "type": "library",
    "description": "Map Larvel Eloquent models to Elasticsearch types.",
    "keywords": ["elasticsearch","eloquent","laravel"],
    "homepage": "https://github.com/adamfairholm/Elasticquent",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Fairholm",
            "email": "adam.fairholm@gmail.com",
            "homepage": "http://adamfairholm.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5",
        "illuminate/database": "~5",
        "nesbot/carbon": "~1",
        "elasticsearch/elasticsearch": "~1|~2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "Elasticquent\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
